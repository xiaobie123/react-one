{
  "presets": [
    "react", 
    ["env",{"modules":false}], 
    "stage-1"
  ],
  "plugins":["transform-decorators-legacy"],
  "env": {
    "dev": {
        "plugins": [
          ["react-transform", {
             "transforms": [{
               "transform": "react-transform-hmr",
               "imports": ["react"],
               "locals": ["module"]
             }]
          }],
      ]
    }
  }
}